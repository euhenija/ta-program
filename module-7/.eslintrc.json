{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["plugin:prettier/recommended"],
  "plugins": [ "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "parser": "babel-eslint",
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "rules": {
    "prettier/prettier": [
        "error",
        {
        "endOfLine": "auto",
        "printWidth": 120,
        "singleQuote": true
        }
      ],
    "no-unused-vars": "error"
  }
}