{
    "env": {
        "browser": true,
        "commonjs": true
    },
    "extends": ["plugin:prettier/recommended"],
    "plugins": [ "prettier", "import"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
            "endOfLine": "auto",
            "printWidth": 120,
            "singleQuote": true
            }
        ],
        "no-unused-vars": "error"
    }
}
